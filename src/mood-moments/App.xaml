<?xml version = "1.0" encoding = "UTF-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:mood_moments"
             x:Class="mood_moments.App"
             xmlns:converters="clr-namespace:mood_moments.Converters">
    <Application.Resources>
        <ResourceDictionary>
            <converters:TrueToVisibilityConverter x:Key="TrueToVisibilityConverter" />
            <converters:FalseToVisibilityConverter x:Key="FalseToVisibilityConverter" />
            <converters:NullToBoolConverter x:Key="NullToBoolConverter" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
